{
    "framework": "nextjs",
    "buildCommand": "npm run build",
    "devCommand": "npm run dev",
    "installCommand": "npm install",
    "regions": ["iad1"],
    "functions": {
      "src/app/api/universal-assistant/tts/route.ts": {
        "maxDuration": 30
      },
      "src/app/api/universal-assistant/transcribe/route.ts": {
        "maxDuration": 30
      },
      "src/app/api/universal-assistant/ai-response/route.ts": {
        "maxDuration": 30
      }
    },
    "env": {
      "NEXT_PUBLIC_APP_URL": "@app_url",
      "FIREBASE_ADMIN_PROJECT_ID": "@firebase_admin_project_id",
      "FIREBASE_ADMIN_CLIENT_EMAIL": "@firebase_admin_client_email",
      "FIREBASE_ADMIN_PRIVATE_KEY": "@firebase_admin_private_key",
      "DEEPGRAM_API_KEY": "@deepgram_api_key",
      "ELEVENLABS_API_KEY": "@elevenlabs_api_key",
      "OPENAI_API_KEY": "@openai_api_key",
      "ANTHROPIC_API_KEY": "@anthropic_api_key"
    }
  }